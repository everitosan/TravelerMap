@font-face
    font-family 'icomoon'
    src url('./assets/icomoon/fonts/icomoon.eot?rboo4f')
    src url('./assets/icomoon/fonts/icomoon.eot?rboo4f#iefix') format('embedded-opentype'),
    url('./assets/icomoon/fonts/icomoon.ttf?rboo4f') format('truetype'),
    url('./assets/icomoon/fonts/icomoon.woff?rboo4f') format('woff'),
    url('./assets/icomoon/fonts/icomoon.svg?rboo4f#icomoon') format('svg')
    font-weight normal
    font-style normal

[class^="icon-"], [class*=" icon-"]
